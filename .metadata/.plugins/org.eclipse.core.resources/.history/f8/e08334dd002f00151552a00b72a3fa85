package cahpter6;

public class Student {
	String name;
	int ban;
	int no;
	int kor;
	int eng;
	int math;
	
	public int getTotal(){
		int temp = kor + eng + math;
		return temp;
	}
	
	public float getAverage(){
		float average = Float.parseFloat(String.format("%.3f", getTotal()/3));
				
		return average;
	}
}
