package chapter7;

 class SutdaCard {
	int num;
	boolean isKwang;
	
	public SutdaCard() {
		this.num = 1;
		this.isKwang = true;
	}

	public SutdaCard(int num, boolean isKwang) {
		this.num = num;
		this.isKwang = isKwang;
	}
	
	public String info() {
		
		String info;
		if(isKwang == true){
			info = num + "K";
		}else{
			info = num + "";
		}
		return info;
	}
	
	
}

public class Excercise7_14 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
