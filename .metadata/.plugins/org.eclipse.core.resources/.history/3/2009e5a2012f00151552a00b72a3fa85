package cahpter6;

public class Student {
	String name;
	int ban;
	int no;
	int kor;
	int eng;
	int math;
	
	public int getTotal(){
		int temp = kor + eng + math;
		return temp;
	}
	
	public float getAverage(){
		float average = this.getTotal()/3;
//		String temp = String.format("%.2f", average);
		return average;
	}
}
