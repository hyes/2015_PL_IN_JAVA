package cahpter6;

public class Student {
	String name;
	int ban;
	int no;
	int kor;
	int eng;
	int math;
	
	public int getTotal(){
		int temp = kor + eng + math;
		return temp;
	}
	
	public float getAverage(){
		float temp = (float)this.getTotal()/(float)3;
		double b;
		b = Math.round(temp * 100.0)/100.0;
		
		return (float) (temp + b);
	}
}
